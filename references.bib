@article{pelletierActivationStateIntegrin1995,
  title = {The {{Activation State}} of the {{Integrin $\alpha$IIb$\beta$3 Affects Outside-in Signals Leading}} to {{Cell Spreading}} and {{Focal Adhesion Kinase Phosphorylation}} *},
  author = {Pelletier, Anthony J. and Kunicki, Thomas and Ruggeri, Zaverio M. and Quaranta, Vito},
  year = {1995},
