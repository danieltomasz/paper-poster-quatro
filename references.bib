@article{miroshnikovaAdhesionForcesCortical2018,
  title = {Adhesion Forces and Cortical Tension Couple Cell Proliferation and Differentiation to Drive Epidermal Stratification},
  author = {Miroshnikova, Yekaterina A. and Le, Huy Q. and Schneider, David and Thalheim, Torsten and R{\"u}bsam, Matthias and Bremicker, Nadine and Polleux, Julien and Kamprad, Nadine and Tarantola, Marco and Wang, Ir{\`e}ne and Balland, Martial and Niessen, Carien M. and Galle, Joerg and Wickstr{\"o}m, Sara A.},
  year = {2018},
